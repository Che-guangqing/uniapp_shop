<view class="wrapper"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="box"><view class="info"><view class="info_row"><text class="info_name">{{item.name}}</text><text class="info_tel">{{item.tel}}</text></view><view class="address">{{''+item.address+''}}</view></view><view class="btn"><view class="btn_row"><view data-event-opts="{{[['tap',[['onSure',[index]]]]]}}" class="flex1" bindtap="__e"><block wx:if="{{flag}}"><image class="btn_sure" src="../../static/my/sure.png" mode></image></block><block wx:else><image class="btn_sure" src="../../static/my/no.png" mode></image></block><text>默认地址</text></view><view data-event-opts="{{[['tap',[['delAdress',[index]]]]]}}" class="del" bindtap="__e"><image class="btn_del" src="../../static/my/del.png" mode></image><text>删除</text></view></view></view></view></block><navigator url="../addAddress/addAddress" open-type="redirect"><button class="add_address" type="warn">添加新地址</button></navigator></view>