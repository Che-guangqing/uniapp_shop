<view><view class="top"></view><view class="nav"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="nav_item"><image class="nav_icon" src="{{item.url}}" mode></image><text class="nav_text">{{item.text}}</text></view></block></view><view class="bg"><image class="blue" src="../../static/shop/icon/蓝.png" mode></image><view class="mid"><image class="yellow" src="../../static/shop/icon/黄_.png" mode></image><image class="green" src="../../static/shop/icon/绿.png" mode></image></view><image class="red" src="../../static/shop/icon/红.png" mode></image></view><view class="iconfont"><block wx:for="{{iconfontList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="iconfont_item"><image class="iconfont_icon" src="{{item.url}}" mode></image><view class="iconfont_text">{{item.text}}</view></view></block></view><view class="bar"><scroll-view class="scroll-tab" scroll-x="true" scroll-with-animation="{{true}}"><block wx:for="{{bars}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['toggleTab',[index]]]]]}}" class="{{['scroll-tab-item',(tabIndex==index)?'active':'']}}" bindtap="__e">{{''+item.name+''}}<view class="scroll-tab-line"></view></view></block></block></scroll-view></view><view class="content"><swiper current="{{tabIndex}}" data-event-opts="{{[['change',[['tabChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{contentList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view>{{item.type+item.typeDes}}</view><view class="swiper_flex"><block wx:for="{{item.cont}}" wx:for-item="item" wx:for-index="index"><view class="swiper_item"><image class="item_img" src="{{item.img}}" mode></image><view class="item_des">{{item.goodsDes}}</view><text class="item_price">{{item.price}}</text><text class="item_num">{{"已有"+item.num+"兑换"}}</text><image class="free_img" src="../../static/shop/icon/免费兑换.png" mode></image></view></block></view></swiper-item></block></swiper></view></view>